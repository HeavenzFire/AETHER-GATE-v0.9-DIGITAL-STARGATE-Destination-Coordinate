{
  "manifest_version": 3,
  "name": "GuardianOS: Quantum Veil",
  "version": "4.1",
  "description": "The complete Sovereign OS. CLI, Audio, and Gesture Sculpting.",
  "permissions": ["storage", "activeTab", "scripting"],
  "content_scripts": [
    {
      "matches": [
        "://.google.com/*",
        "://.youtube.com/*",
        "://.gmail.com/*",
        "://.gstatic.com/*",
        "://.googleusercontent.com/*"
      ],
      "js": ["content.js"],
      "run_at": "document_start"
    }
  ]
}